PROTO sub_tecto []{

	Transform {
	
		children [
				
			Transform {
				translation	-2.8 0 0 
				children lateral {}
			}


			Transform {
				translation 2.8 0 0 
				children lateral {}
			}

				Transform {
				translation 0 0 3.25 
				children lateral_fundo {}
			}

				Transform {
				translation 0 0 -3.45 
				children lateral_fundo {}
			}


			Transform {
				 translation  0 0 1.1 
				children	teste{}

		}
				
			Transform {
				scale 0.1 0.1 0.1
				translation	0 -0.1 0
				children alarme {}

		}


		Transform {
			translation	0 0 -0.09
			children meio8{}
		}
	]
	
	}
	
}


PROTO Tecto_completo[]{
	Group {
		children [
			sub_tecto {}

			Transform {
				rotation 0 1 0 3.14159265
				translation	0 0 7.5
				children sub_tecto {}
			}
		]
	}
}

 


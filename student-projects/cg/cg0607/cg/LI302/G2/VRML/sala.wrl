#VRML V2.0 utf8
Background { skyColor 0 0 1 groundColor 0 0.4 0 }

# ***********************************************************************************
# **************************************** AXIS *************************************
# ***********************************************************************************

PROTO myAx [ field SFColor axisColor 0 1 0 ]{
	Group {
		children [
			Transform {  
				translation 0 2 0
				rotation 0 0 0 0
				children[ 
					Shape { 
						geometry Cone{
							bottomRadius 0.05
							height 0.25
							side TRUE
							bottom TRUE
						}#<- geometry
						appearance Appearance { 
							material DEF coneColor Material { diffuseColor IS axisColor } 
						}#<- apearence
					}#<- shape
				]#<- children
			}#<- transform
			Transform{
				translation 0 1 0
				rotation 0 0 0 0
				children [ 
					Shape{ 
						geometry  Cylinder { # ax
							radius 0.025
							side TRUE
							bottom TRUE
							top TRUE
						}#<- geometry
						appearance Appearance { 
							material USE coneColor
						}#<- appearence
					}#<- shape
					Transform {
						scale	1 3 1
						children	[
							Shape	{
								geometry	Cylinder	{ # keeping ax
									radius 0.003
								}#<- geometry
								appearance Appearance { material USE coneColor	}#<- appearence
							}#<- shape
						]#<- children
					}#<- transform
				]#<- children
			}#<- transform
			#sphere / center
			Shape {
				geometry Sphere	{ radius 0.05 }#<- geometry
				appearance Appearance { 
					material DEF mat Material { diffuseColor 1 1 0 } #<- material
				}#<- appearence
			}#<- shape
		]#<- children
	}#<- group	
}#<- proto

PROTO myText [  
		field MFString label "X"
		field SFColor  txtColor 1 0 0 
		field SFVec3f  trans 0 0 0
		field SFRotation rot 0 0 0 0]{
	Transform {
		translation	IS trans
		rotation	IS	rot
		children [
			Shape {
				geometry Text {
					string IS label
					length 2
					fontStyle  FontStyle {
						family "Monotype Corsiva"
						horizontal TRUE
						language ""
						size 0.5
						leftToRight	TRUE
						style "BOLD"
						topToBottom	FALSE
					}#<- fontstyle
				}#<- geometry
				appearance Appearance { 
					material DEF col Material { diffuseColor IS txtColor } 
				}#<- apearence
			}#<- shape
		]#<- children
	}#<- transform
}#<- proto

PROTO myAxis []{
	Group {
		children [
			Transform {	
				children [
				   myAx {}#<- myAx
					myText {  
						label "Y"
				  		txtColor 0 1 0 
						trans 0.1 2.3 0 
					}#<- mytext
				]#<- children
			}#<- transform
			Transform {
				rotation 0 0 1 -1.57
				children	[  
					myAx { axisColor 1 0 0 }#<- myAx
					myText { 
						label "X"
						txtColor 1 0 0
						trans  0 2 0 
						rot 0 0 1 1.57 
					}#<- myText
				]#<- children
			}#<- transform
			Transform {
				rotation 1 0 0 1.57
				children	[ 
					myAx { axisColor 0 0 1 }#<- myAx
					myText { 
						label "Z"
						txtColor 0 0 1
						trans 0 2 0 
						rot 1 0 0 -1.57 
					}#<- myText
				]#<- children
			}#<- transform
		]#<- children
	}#<- group
}#<- proto

# ***********************************************************************************
# **************************************** WALLS ************************************
# *********************************************************************************** 

PROTO walls
[
	field SFVec3f trans 0 0 0
	field SFNode points Coordinate 
	{
		point 
		[
			3.50 0.00  4.40 , 3.50 3.00  4.40 , -3.50 3.00  4.40  , -3.50 0.00  4.40  , # 0 - 3
			3.40 0.00  4.40 , 1.90 0.00  4.40 ,  1.90 2.55  4.40  ,  3.40 2.55  4.40  , # 4 - 7
			3.50 0.00 -4.40 , 3.50 3.00 -4.40 , -3.50 3.00 -4.40  , -3.50 0.00 -4.40 , # 8 - 11
		]
	}
]
{
	Transform {
		children Shape 
			{	
				geometry IndexedFaceSet { color Color { 
					color 
					[
						0 0 .4 , .8 .8 .8 ,
					   0 0 .4 , .8 .8 .8 , 
						0 0 .4 , .8 .8 .8 ,
						0 0 .4 , .8 .8 .8 ,
						0 0 .4 , .9 .9 .9 ,
						0 0 .4 , .4 .4 .4 
					]
					
				}
				
				coord IS points
				coordIndex 
				[ 
					0 1 2 3 5 6 7 4	   -1		0 4 7 6 5 3 2 1		-1 ,# frente fora & dentro
					11 10 9 8				-1		8 9 10 11				-1 ,# tras dentro & fora
					8 9 1 0					-1		0 1 9 8					-1	,# lado dentro & fora (direito)
					3 2 10 11				-1		11 10 2 3				-1	,# lado dentro & fora (esquerdo)
					1 9 10 2					-1		2 10 9 1					-1	,# cima
					3 11 8 0					-1		0 8 11 3					-1	,# baixo
				]
				colorPerVertex FALSE
			}
		}
		translation IS trans
	}
}


PROTO funcaoBox[  field	SFVec3f escala 1 1 1
				  field SFRotation rotacao 0 0 0 0,
				  field SFVec3f translacao 0 0 0,
				  field SFColor cor 0.8 0.8 0.8,
				  field	SFVec3f	dimensoes 2 2 2
				  field	MFString imagemTextura ""
				  field	SFVec2f	escalaTextura 1 1
			  ]{
	Transform	{
		scale IS escala
		rotation IS	rotacao
		translation	IS translacao
		children [
			Shape {
				appearance Appearance {
					material Material {diffuseColor IS	cor}
					texture	ImageTexture {url IS imagemTextura}
					textureTransform TextureTransform {scale IS escalaTextura }
				}
				geometry Box {
					size IS	dimensoes
				}
			}
			
		]
	}		  
}


PROTO funcaoBoxMetal[  
				  field	SFVec3f escala 1 1 1
				  field SFRotation rotacao 0 0 0 0,
				  field SFVec3f translacao 0 0 0,
				  field	SFVec3f	dimensoes 2 2 2
			  ]{
	Transform	{
		scale IS escala
		rotation IS	rotacao
		translation	IS translacao
		children [
			Shape {
				appearance Appearance {
					material Material {
						diffuseColor .61 .61 .61
						specularColor .39 .39 .39
						ambientIntensity .25
						shininess .11
				} 					
				}
				geometry Box {
					size IS	dimensoes
				}
			}
			
		]
	}		  
}


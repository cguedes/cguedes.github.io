			Transform {
				scale IS Escala
				children [					
				vFieldMarks  {}
				]
			}

			#lado mais perto (z)
			Transform {
				scale IS Escala
				translation	0 0 20
				children [					
				vFieldMarks  {}
				]
			}

			#lado mais longe (z)
			Transform {
				scale IS Escala
				translation	0 0 -20
				children [					
				vFieldMarks  {}
				]
			}

			#canto esquerdo
			Transform {
				rotation 0 1 0 1.57
				scale 40 1 0.08
				translation	-10 0 0
				children [					
				vFieldMarks  {}
				]
			}


			#canto direito
			Transform {
				rotation 0 1 0 1.57
				scale 40 1 0.08
				translation	10 0 0
				children [					
				vFieldMarks  {}
				]
			}

			#Marcações
			Transform {
				children [					
				vMarkings  {}
				]
			}


			#Baliza longe

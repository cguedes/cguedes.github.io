PROTO vMarkings
[
]
{
Transform {	
	children [
		# meia lua de perto
		Transform {	
		translation	0 0 20	
			scale 8 0.01 6
			children [					
				vHalfMark {
					transparency 0
					url	"images/LinhaMarcacao.png"
				}
				]
		}

		# meia lua tracejada de perto
		Transform {						
			translation	0 0 15	
			scale 10.02 0.02 6
			children [				

				vHalfMark {							
					scale 30 30
					url	"images/LinhaMarcacao2.png"
				}
				]
		}

		# meia lua de longe
		Transform {	
		rotation 0 1 0 -3.1415
		translation	0 0 -20	
			scale 8 0.01 6
			children [
				
				vHalfMark {}
				]
		}

		# meia lua tracejada de longe
		Transform {			
			rotation	0 1 0 -3.1415		
			translation	0 0 -15	
			scale 10.02 0.02 6
			children [				

				vHalfMark {							
					scale 30 30
					url	"images/LinhaMarcacao2.png"
				}
				]
		}

	]
}

}

PROTO vPoste [
	exposedField SFVec2f scale 1 8
	field SFFloat height 2
	exposedField MFString url ["images/BocadoBaliza.png"]
	field SFFloat radius 0.05
	exposedField SFColor diffuseColor 0 0 0
]
{
	Transform {
		children [
			Shape {
				geometry Cylinder {
					height IS height
					radius IS radius
				}

				appearance Appearance {
					material Material {
						#shininess 1
						emissiveColor 0 0 0
						#diffuseColor 0 0 0
						#diffuseColor IS	diffuseColor
					}

					texture	ImageTexture {
						url	IS url
						repeatS	TRUE
					}

					textureTransform TextureTransform {
						scale IS scale
					}
					
				}
			}
		]
		
}

}




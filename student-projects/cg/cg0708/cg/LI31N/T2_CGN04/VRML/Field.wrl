		scale	24 1 44
		children 
			Shape {
	  	  
    	appearance Appearance {
			texture	ImageTexture {
					url "images/ChaoCampo.png"
				}
        	material Material { 
				#diffuseColor 0.3 1 0.3
				specularColor 0 0.1 0
				
			}           												        		
				

PROTO portaTunel[field SFVec3f tamanho 1 1 1, field SFVec3f posicao 0 0 0, field SFRotation rotacao 0 0 0 0]{ #tamanho 1.1 2.4 0.035
	DEF	portaTunelDef
	Transform {
		scale	IS	tamanho
		rotation IS	rotacao
		translation	IS posicao
	    children 	
		[				
			Transform 
			{
				scale 0.97 1.165 1
				translation	0.50 1.3825 0
				children janelaSala{}
			}

			Transform {
				translation	0.5 0.4 0
				children Shape {
					appearance Appearance { 					 
						material Material {}
					}
					geometry Box{
						size 1 0.8 0.035
					}
				}
			}
			Transform {
				translation	0.0175 1.4 0
				children Shape{
					appearance Appearance { 					 
						material Material {}
					}
				    geometry Box {
						size 0.035 1.2 0.035
					}
				}
			}
			Transform {
				translation	0.9825 1.4 0
				children Shape {
					appearance Appearance { 					 
						material Material {}
					}
					geometry Box {
						size 0.035 1.2 0.035
					}
				}
			}
			Transform {
				translation	0.50 1.9825 0
				children Shape {
					appearance Appearance { 					 
						material Material {}
					}
					geometry Box {
						size 1 0.035 0.035
					}
				}
			}

			DEF DoorSense ProximitySensor 
			{
            	center 0 1 0
            	size 3 3 3
            }
			
			DEF DoorSenseClose ProximitySensor 
			{
            	center 0 1 0
            	size 3.5 3 3.5
            }
			DEF	openClock TimeSensor
			{
				cycleInterval 1.5
			}
			DEF	closeClock TimeSensor
			{
				cycleInterval 1.5
				
			}
			DEF	rotOpen OrientationInterpolator
			{
				key[0, 1]
				keyValue [0 1 0 0, 0 1 0 1.57]
			}
			DEF	rotClose OrientationInterpolator
			{
				key[0, 1]
				keyValue [0 1 0 1.57, 0 1 0 0]
			}
				
		]

		ROUTE DoorSense.enterTime TO openClock.set_startTime
		ROUTE DoorSense.exitTime TO openClock.set_stopTime

		ROUTE DoorSenseClose.exitTime TO closeClock.set_startTime
		ROUTE DoorSenseClose.enterTime TO closeClock.set_stopTime


		ROUTE openClock.fraction_changed TO	rotOpen.set_fraction
		ROUTE closeClock.fraction_changed TO rotClose.set_fraction
		ROUTE rotOpen.value_changed	TO portaTunelDef.set_rotation
		ROUTE rotClose.value_changed TO	portaTunelDef.set_rotation
	}
}


#Routes	& script

DEF switch Script {
	field SFBool VarBool TRUE
	field MFString imgArray ["nigthSky.jpg", "Clouds.jpg"]
	eventIn	SFBool Set_true_false
	eventOut SFBool	VarOn
	eventOut SFBool	VarOff
	eventOut MFString img
	url	"javascript:
	function Set_true_false(v, timestamp){
		if(v==true)	 return;
		if (VarBool==false)	 {
			VarBool=true;
			img = new MFString(imgArray[1]);
		}
		else{
		    VarBool=false;
			img=new MFString(imgArray[0])
		}
		VarOn=VarBool
		VarOff=!VarBool

	}"
}



ROUTE LightTouch.isActive    TO    switch.Set_true_false
